<test>
  <assessmentItem identifier="choice" title="Basic Maths" adaptive="false" timeDependent="false">
    <responseDeclaration identifier="RESPONSE" cardinality="single" baseType="identifier">
      <correctResponse>
        <value>ChoiceA</value>
      </correctResponse>
    </responseDeclaration>
    <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float">
      <defaultValue>
        <value>0</value>
      </defaultValue>
    </outcomeDeclaration>
    <itemBody>
      <p>This is a simple maths question.</p>
      <choiceInteraction responseIdentifier="RESPONSE" shuffle="false" maxChoices="1">
        <prompt>what is 5 x 2?</prompt>
        <simpleChoice identifier="ChoiceA">10</simpleChoice>
        <simpleChoice identifier="ChoiceB">3</simpleChoice>
        <simpleChoice identifier="ChoiceC">7</simpleChoice>
      </choiceInteraction>
    </itemBody>
    <responseProcessing/>
  </assessmentItem>
</test>